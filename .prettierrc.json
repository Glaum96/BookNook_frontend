{
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
  "$schema": "http://json.schemastore.org/prettierrc",
  "printWidth": 120,
  "parser": "typescript",
  "useTabs": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "semi": false,
  "arrowParens": "always",
  "endOfLine": "auto"
}
